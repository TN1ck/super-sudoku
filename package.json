{
  "name": "super-sudoku",
  "version": "0.1.0",
  "description": "Super Sudoku",
  "license": "MIT",
  "scripts": {
    "build": "react-static build",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watchAll": "jest --watchAll",
    "start": "react-static start",
    "prettier:write": "prettier --write './src/**/*.ts' './src/**/*.tsx'",
    "generate-easy-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d easy -n 20",
    "generate-medium-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d medium -n 20",
    "generate-hard-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d heard -n 20",
    "generate-evil-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d evil -n 20"
  },
  "keywords": [
    "super sudoku",
    "online sudoku",
    "sudoku",
    "game"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "resolutions": {
    "react-static/webpack-dev-server": "3.2.1"
  },
  "devDependencies": {
    "@types/d3-path": "1.0.8",
    "@types/jest": "24.0.11",
    "@types/keymaster": "1.6.28",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.5",
    "@types/react": "16.8.13",
    "@types/react-redux": "7.0.7",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "4.1.5",
    "@types/webpack-env": "1.13.9",
    "commander": "2.20.0",
    "eslint": "5.16.0",
    "file-loader": "3.0.1",
    "imports-loader": "0.8.0",
    "jest": "24.7.1",
    "json-loader": "0.5.7",
    "raw-loader": "2.0.0",
    "react-static": "7.0.9",
    "react-static-plugin-styled-components": "7.0.8",
    "react-static-plugin-typescript": "7.0.8",
    "showdown": "1.9.0",
    "source-map-support": "0.5.12",
    "ts-helpers": "1.1.2",
    "ts-jest": "24.0.2",
    "ts-loader": "5.3.3",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "4.0.0",
    "typescript": "3.3.3",
    "url-loader": "1.1.2"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/react-lazyload": "^2.3.2",
    "babel-polyfill": "6.26.0",
    "history": "4.9.0",
    "keymaster": "1.6.2",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.4",
    "react-lazyload": "^2.5.0",
    "react-redux": "7.0.2",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "4.2.0"
  }
}
