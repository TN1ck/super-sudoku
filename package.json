{
  "name": "typescript2-react-redux-cssmodules-boilerplate",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript2/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean && export GENERATE_TYPINGS=TRUE && node build.js",
    "build": "export GENERATE_TYPINGS=FALSE && NODE_ENV=production && node build.js",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watchAll": "jest --watchAll",
    "start": "node server.js"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "hot",
    "starter",
    "seed"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@types/classnames": "0.0.32",
    "@types/jest": "^18.1.1",
    "@types/lodash": "^4.14.52",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.12",
    "@types/react-css-modules": "^3.7.6",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router": "^3.0.4",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^6.3.3",
    "awesome-typescript-loader": "^3.0.8",
    "babel-core": "^6.23.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "classnames": "^2.2.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "cssnano": "^3.5.2",
    "eslint": "^3.2.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.7.1",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-modules-local-by-default": "^1.0.2",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.4.0",
    "stylelint-order": "^0.3.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^19.0.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.0.3",
    "typings": "^2.1.0",
    "typings-for-css-modules-loader": "^1.5.0",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "url-loader": "^0.5.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-split-by-path": "2.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
