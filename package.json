{
  "name": "typescript2-react-redux-cssmodules-boilerplate",
  "version": "0.1.0",
  "description": "Super Sudoku",
  "license": "MIT",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "build": "react-static build",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watchAll": "jest --watchAll",
    "start": "react-static start",
    "prettier:base": "prettier --parser typescript --single-quote --trailing-comma=\"all\" --no-bracket-spacing",
    "prettier:check": "npm run prettier:base -- --list-different './src/**/*.ts' './src/**/*.tsx'",
    "prettier:write": "npm run prettier:base -- --write './src/**/*.ts' './src/**/*.tsx'",
    "generate-easy-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d easy -n 5"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "hot",
    "starter",
    "seed"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/d3-path": "^1.0.6",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.71",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/redux-logger": "^3.0.0",
    "@types/webpack-env": "^1.13.0",
    "commander": "2.18.0",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "eslint": "5.5.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "raw-loader": "0.5.1",
    "react-static": "5.9.8",
    "showdown": "^1.8.6",
    "source-map-support": "0.5.9",
    "style-loader": "0.23.0",
    "stylelint": "9.5.0",
    "stylelint-order": "1.0.0",
    "stylelint-webpack-plugin": "0.10.5",
    "ts-helpers": "1.1.2",
    "ts-jest": "23.1.4",
    "ts-loader": "3.5.0",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "^3.1.0",
    "typescript": "3.0.3",
    "ts-node": "^7.0.1",
    "url-loader": "1.1.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "3.1.8"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "2.2.6",
    "history": "4.7.2",
    "lodash": "4.17.10",
    "react": "16.5.0",
    "react-hot-loader": "4.3.6",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "^3.4.6"
  }
}
