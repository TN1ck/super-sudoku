    "lodash": "4.17.15",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-lazyload": "^2.6.5",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
