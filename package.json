{
  "name": "super-sudoku",
  "version": "0.1.0",
  "description": "Super Sudoku",
  "license": "MIT",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "build": "react-static build",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watchAll": "jest --watchAll",
    "start": "react-static start",
    "prettier:write": "prettier --write './src/**/*.ts' './src/**/*.tsx'",
    "generate-easy-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d easy -n 10",
    "generate-evil-sudokus": "ts-node --project ./tsconfig.json ./scripts/generate_sudokus.ts -d evil -n 20"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "hot",
    "starter",
    "seed"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/d3-path": "^1.0.6",
    "@types/jest": "^23.3.2",
    "@types/keymaster": "^1.6.28",
    "@types/lodash": "^4.14.71",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-logger": "^3.0.0",
    "@types/styled-components": "^4.1.0",
    "@types/webpack-env": "^1.13.0",
    "commander": "2.19.0",
    "eslint": "^5.12.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest": "23.6.0",
    "json-loader": "0.5.7",
    "raw-loader": "^1.0.0",
    "react-static": "^6.2.0",
    "react-static-plugin-styled-components": "^6.1.1",
    "react-static-plugin-typescript": "^3.0.1",
    "showdown": "^1.8.6",
    "source-map-support": "0.5.9",
    "ts-helpers": "1.1.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-loader": "3.6.0",
    "tslint-react": "^3.1.0",
    "typescript": "^3.2.2",
    "url-loader": "1.1.2",
    "webpack": "^4.28.4",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "4.7.2",
    "keymaster": "^1.6.2",
    "lodash": "4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "^4.1.1"
  }
}
